#!/usr/bin/env perl
use v5.42.0;
use utf8::all;
use File::FindLib 'lib';
use Devel::Local;
use Carp;

require App::Schierer::HPFan::Model::Gedcom;
require App::Schierer::HPFan::Logger::Config;
